all : mapWidget.py dashboardDialog.py sequenceLandDialog.py fixedFormationDialog.py

#Careful! Don't remove __init__.py or any .py not generated by this Makefile
clean :
	rm mapWidget.py
	rm dashboardDialog.py
	rm sequenceLandDialog.py
	rm fixedFormationDialog.py

mapWidget.py : mapWidget.ui
	pyuic5 mapWidget.ui -o mapWidget.py

dashboardDialog.py : dashboardDialog.ui
	pyuic5 dashboardDialog.ui -o dashboardDialog.py

sequenceLandDialog.py : sequenceLandDialog.ui
	pyuic5 sequenceLandDialog.ui -o sequenceLandDialog.py

fixedFormationDialog.py : fixedFormationDialog.ui
	pyuic5 fixedFormationDialog.ui -o fixedFormationDialog.py


